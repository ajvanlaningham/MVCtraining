@model IEnumerable<MVCtraining.Models.ApplicationType>

<div class="containter p-3">
    <div class="row" pt-4">
        <div class="col-6</div>">
            <h2 class="text-primary">Application Types</h2>
        </div>
        <div class="col-6 text-right">
            <a asp-action="Create" class="btn btn-primary">
                Create New Application Type
            </a>
        </div>
     </div>

    <br /><br />

    @if(Model.Count() >0)
     {
        <table class="table table-bordered table-striped" style="width:100%">
            <thread>
                <tr>
                    <th>
                     Name
                    </th>
                    
                    <th></th>
                </tr>
            </thread>
            <tbody>
           @foreach(var obj in Model)
            {
            <tr>
                <td width="50%">@obj.Name</td>
                
                <td></td>
            </tr>
            }
            </tbody>
        </table>
            }
            else
            {
                <p>Not Application Types exists.</p>
            }
</div>
